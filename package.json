{
  "name": "salahor",
  "version": "0.3.0",
  "description": "Zero-dependency universal connectors between Events, EventTargets and AsyncIterables with lightweight operators.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    },
    "./operators": {
      "types": "./src/operators/index.d.ts",
      "import": "./src/operators/index.js"
    },
    "./sources": {
      "types": "./src/sources/index.d.ts",
      "import": "./src/sources/index.js"
    },
    "./utils": {
      "types": "./src/utils/index.d.ts",
      "import": "./src/utils/index.js"
    },
    "./core": {
      "types": "./src/core/index.d.ts",
      "import": "./src/core/index.js"
    },
    "./workers": {
      "types": "./src/workers/index.d.ts",
      "import": "./src/workers/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "events",
    "eventtarget",
    "async-iterable",
    "stream",
    "connector",
    "zero-dependency",
    "reactive",
    "observable",
    "async",
    "worker",
    "webworker"
  ],
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "author": "Gzeu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gzeu/salahor.git"
  },
  "bugs": {
    "url": "https://github.com/Gzeu/salahor/issues"
  },
  "homepage": "https://github.com/Gzeu/salahor#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "node -e \"console.log('salahor ready')\"",
    "test": "node test/modular-test.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2"
  },
  "dependencies": {
    "salahor": "^0.3.0",
    "ws": "^8.18.3"
  }
}
